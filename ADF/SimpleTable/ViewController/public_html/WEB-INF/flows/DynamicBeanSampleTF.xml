<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="DynamicBeanSampleTF">
    <default-activity>DynamicBeanSample</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>dynamicBean</managed-bean-name>
      <managed-bean-class>com.example.adf.view.bean.DynamicBeanSample</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>customDynamicBean</managed-bean-name>
      <managed-bean-class>com.example.adf.view.bean.GenericDynamicBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="DynamicBeanSample">
      <page>/DynamicBeanSample.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
