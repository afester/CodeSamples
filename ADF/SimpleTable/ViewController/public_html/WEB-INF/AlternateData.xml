<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="AlternateData">
    <default-activity>AddressTable</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="AddressTable">
      <page>/AddressTable.jsff</page>
    </view>
    <method-call id="path1"></method-call>
    <method-call id="path2"></method-call>
    <control-flow-rule id="__1">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>form</from-outcome>
        <to-activity-id>path1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>table</from-outcome>
        <to-activity-id>path2</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>path1</from-activity-id>
      <control-flow-case id="__5">
        <to-activity-id>AddressTable</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>path2</from-activity-id>
      <control-flow-case id="__7">
        <to-activity-id>AddressTable</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
